{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsAngularService",
  "title": "Angular Service Options Schema",
  "type": "object",
  "properties": {
    "url": {
      "type": "string",
      "description": "The url of the service."
    },
    "name": {
      "type": "string",
      "description": "The name of the service."
    },
    "path": {
      "type": "string",
      "description": "The path of the service."
    },
    "entity": {
      "type": "string",
      "description": "The entity name of the service."
    },
    "project": {
      "type": "string",
      "description": "The name of the project.",
      "$default": {
        "$source": "projectName"
      }
    }
  },
  "required": [
    "url",
    "name"
  ]
}